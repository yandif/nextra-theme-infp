{
  "name": "nextra-theme-infp",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/",
    "lib/",
    "package.json",
    "style.css",
    "README.md"
  ],
  "keywords": [
    "nextra",
    "nextra theme",
    "mantine",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yandif/nextra-theme-infp.git"
  },
  "author": {
    "name": "yandif",
    "email": "me.yandif@gmail.com",
    "url": "https://yandif.com"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git add . && git-cz && git push",
    "build:theme": "rimraf ./lib && rimraf ./dist && rimraf ./style.css && vite build && pnpm build:css && pnpm build:lib",
    "build:lib": "tsc -p ./tsconfig.build.json",
    "build:css": "pnpm postcss src/styles/index.css -o style.css --verbose --minify"
  },
  "dependencies": {
    "@legendapp/state": "^2.1.4",
    "@mantine/core": "7.5.0",
