{
  "name": "nextra-theme-infp",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "sideEffects": false,
  "author": "Yandif <me.yandif@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/yandif/nextra-themes.git",
    "type": "git",
    "directory": "packages/nextra-theme-infp"
  },
  "keywords": [
    "react",
    "nextra",
    "nextra-theme"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf ./lib && rimraf ./dist && rimraf ./style.css &&  pnpm build:lib && vite build && pnpm build:tailwind",
    "build:lib": "tsc -p ./tsconfig.build.json && tsc-alias -p ./tsconfig.build.json",
    "build:tailwind": "pnpm postcss src/style.css -o style.css --verbose"
  },
  "dependencies": {
    "@legendapp/state": "^1.11.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lodash": "^4.17.21",
    "next": "^13.5.3",
    "nextra": "^2.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "tsc-alias": "^1.8.8",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  }
}
